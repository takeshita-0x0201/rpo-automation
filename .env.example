# Google Cloud Project ID
GOOGLE_CLOUD_PROJECT=rpo-automation

# API Keys
OPENAI_API_KEY=your-openai-api-key
GEMINI_API_KEY=your-gemini-api-key

# Google Sheets ID for output
GOOGLE_SHEETS_ID=your-google-sheets-id

# SECRET_KEY - FastAPI用のシークレットキー
SECRET_KEY=your-secret-key-generate-with-openssl-rand-base64-32

# JWT認証用
JWT_SECRET_KEY=your-jwt-secret-key

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# WebApp設定
WEBAPP_PORT=8000

# Google Service Account (オプション - gcloud authを使う場合は不要)
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/your-service-account-key.json

# Pub/Sub設定
PUBSUB_TOPIC=scraping-requests
PUBSUB_SUBSCRIPTION=scraping-requests-sub
PUBSUB_RESULT_TOPIC=scraping-results

# スクレイピング認証情報
BIZREACH_USERNAME=your-bizreach-username
BIZREACH_PASSWORD=your-bizreach-password

# データベース接続 (オプション)
DATABASE_URL=postgresql://user:password@localhost/dbname
REDIS_URL=redis://localhost:6379

# GAS API認証用
GAS_API_KEY=your-gas-api-key

# AIマッチング機能用
TAVILY_API_KEY=your-tavily-api-key

# ベクトル検索用
PINECONE_API_KEY=your-pinecone-api-key
PINECONE_INDEX_HOST=your-index-host.pinecone.io

# Google Apps Script Webhook
GAS_WEBHOOK_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# デフォルトログイン情報（開発環境用）
# 本番環境では必ず変更してください
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin1234
USER_USERNAME=user
USER_PASSWORD=user1234

# Docker Compose PostgreSQL設定（開発環境用）
# 本番環境ではSupabaseを使用
POSTGRES_USER=rpo_user
POSTGRES_PASSWORD=your-secure-postgres-password
POSTGRES_DB=rpo_automation_dev